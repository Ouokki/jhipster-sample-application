<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-latest.xsd">
    <!--
        Added the constraints for entity OffreProduit.
    -->
    <changeSet id="20240312104916-2" author="jhipster">

        <addForeignKeyConstraint baseColumnNames="parametrage_id"
                                 baseTableName="offre_produit"
                                 constraintName="fk_offre_produit__parametrage_id"
                                 referencedColumnNames="id"
                                 referencedTableName="parametrage"
                                 />

        <addForeignKeyConstraint baseColumnNames="cr_id"
                                 baseTableName="offre_produit"
                                 constraintName="fk_offre_produit__cr_id"
                                 referencedColumnNames="id"
                                 referencedTableName="cr"
                                 />

        <addForeignKeyConstraint baseColumnNames="offre_id"
                                 baseTableName="offre_produit"
                                 constraintName="fk_offre_produit__offre_id"
                                 referencedColumnNames="id"
                                 referencedTableName="offre"
                                 />

        <addForeignKeyConstraint baseColumnNames="produit_id"
                                 baseTableName="offre_produit"
                                 constraintName="fk_offre_produit__produit_id"
                                 referencedColumnNames="id"
                                 referencedTableName="produit"
                                 />
    </changeSet>
</databaseChangeLog>
